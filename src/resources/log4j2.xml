<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
    <File name="LogFile" fileName="last.log">
        <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </File>
</Appenders>
<Loggers>
    <Logger name="com.denvys5" level="trace" additivity="false">
        <AppenderRef ref="LogFile"/>
    </Logger>
    <Root level="error">
        <AppenderRef ref="LogFile"/>
    </Root>
</Loggers>
</Configuration>